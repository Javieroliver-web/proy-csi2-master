{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./", 
    "ignoreDeprecations": "6.0",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "experimentalDecorators": true,
    "paths": {
      
      "@shared": [ "projects/shared/src/public-api.ts" ],
      "@shared/*": [ "projects/shared/src/lib/*" ]
    },
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022", 
    "moduleResolution": "bundler", 
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "typeCheckHostBindings": true,
    "strictTemplates": true
  },
  "files": [],
  "references": [
    
    {
      "path": "./projects/tienda-informatica/tsconfig.app.json"
    },
    {
      "path": "./projects/tienda-informatica/tsconfig.spec.json"
    },
    {
      "path": "./projects/admin/tsconfig.app.json"
    },
    {
      "path": "./projects/shared/tsconfig.lib.json"
    },
    {
      "path": "./projects/shared/tsconfig.spec.json"
    },
    {
      "path": "./projects/publicaciones/tsconfig.app.json"
    },
    {
      "path": "./projects/publicaciones/tsconfig.spec.json"
    },
    {
      "path": "./projects/fotos-app/tsconfig.app.json"
    },
    {
      "path": "./projects/fotos-app/tsconfig.spec.json"
    }
  ]
}